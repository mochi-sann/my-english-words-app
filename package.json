{
  "name": "next-js-templete",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --fix --ext ts --ext tsx --ext js",
    "test": "jest",
    "server": "yarn build && yarn start",
    "storybook": "start-storybook -s ./public -p 6006",
    "storybook:build": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "storybook:gh": " storybook-to-ghpages --branch dock --source-branch main --out docs"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "push-hook": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "git add ."
    ]
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn format",
      "git add ."
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^1.1.5",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9",
    "awesome-typescript-loader": "^5.2.1",
    "axios": "^0.21.1",
    "dayjs": "^1.10.3",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "final-form": "^4.20.1",
    "final-form-arrays": "^3.0.2",
    "firebase": "^8.2.1",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "framer-motion": "^3.2.1",
    "next": "latest",
    "next-pwa": "^5.0.4",
    "postcss": "^7",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.5.2",
    "react-final-form-arrays": "^3.1.3",
    "react-firebase-hooks": "^2.2.0",
    "react-hook-form": "^6.14.0",
    "react-icons": "^4.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@hookform/devtools": "^2.2.1",
    "@storybook/addon-a11y": "^6.1.14",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-controls": "^6.1.14",
    "@storybook/addon-essentials": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@storybook/storybook-deployer": "^2.8.7",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.3",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.16",
    "@types/node": "^12.12.21",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
